{
  "name": "@jgarber/templatetemplate",
  "version": "2.0.1",
  "description": "A very small JavaScript <template> manipulation library.",
  "keywords": [
    "dom",
    "dom-library",
    "dom-manipulation",
    "javascript",
    "queryselector",
    "template"
  ],
  "homepage": "https://github.com/jgarber623/TemplateTemplate",
  "bugs": "https://github.com/jgarber623/TemplateTemplate/issues",
  "license": "MIT",
  "author": {
    "name": "Jason Garber",
    "email": "jason@sixtwothree.org",
    "url": "https://sixtwothree.org"
  },
  "files": [
    "dist/*",
    "example/*",
    "src/*"
  ],
  "main": "dist/templatetemplate.js",
  "browser": "dist/templatetemplate.min.js",
  "module": "dist/templatetemplate.mjs",
  "directories": {
    "example": "example"
  },
  "repository": "github:jgarber623/TemplateTemplate",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint .",
    "pretest": "npm run build",
    "start": "concurrently --kill-others-on-fail --prefix-colors yellow,blue 'npm:start:*'",
    "start:build": "npm run build -- --watch",
    "start:serve": "http-server .",
    "test": "jasmine-browser-runner runSpecs"
  },
  "devDependencies": {
    "@jgarber/eslint-config": "^1.1.1",
    "@rollup/plugin-terser": "^0.4.3",
    "concurrently": "^8.2.1",
    "globals": "^13.22.0",
    "http-server": "^14.1.1",
    "jasmine-browser-runner": "^2.3.0",
    "jasmine-core": "^5.1.1",
    "rollup": "^3.29.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
